#!/usr/bin/env ruby

W = 25
T = 6

input = $stdin.readlines[0].strip.chars.map(&:to_i)

layers = input.each_slice(W * T).to_a

image = layers.pop

layers.reverse.each do |layer|
  layer.each_index do |i|
    case layer[i]
    when 0
      image[i] = 0
    when 1
      image[i] = 1
    end
  end
end

puts image.map{|p|p == 0 ? " " : "â–ˆ"}.each_slice(W).map(&:join)
